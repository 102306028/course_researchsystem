<%= form_for @course do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :teacher %>
    <%= f.text_field :teacher %>
  </div>
  <div>
    <%= f.label :classroom %>
    <%= f.text_field :classroom %>
  </div>
  <div>
    <%= f.label :time %>
    <%= f.text_field :time %>
  </div>
  <div>
    <%= f.label :score %>
    <%= f.text_field :score %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>